"
Je presente le par pour une donne
"
Class {
	#name : 'Par',
	#superclass : 'Object',
	#instVars : [
		'fmtPBN'
	],
	#category : 'Bridge-DDS',
	#package : 'Bridge-DDS'
}

{ #category : 'accessing' }
Par >> fmtPBN [
	^ fmtPBN
]

{ #category : 'accessing' }
Par >> fmtPBN: anObject [
	fmtPBN := anObject
]
